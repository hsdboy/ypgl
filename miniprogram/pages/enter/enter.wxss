.search{
  width: 100%;
  height: 70rpx;
  background: #eeeeee;
  padding-top:20rpx; 
}
.content{
  height: 90vh;
  overflow: auto;
}
.search input{
  width: 90%;
  margin: 0 auto;
  background: #fefefe;
  text-align: center;
  font-size:35rpx; 
}
.type{
  display: flex;
  justify-content: space-around;
  line-height: 70rpx;
  font-size: 35rpx;
}
.item{
  height: 120rpx;
  font-size: 30rpx;
  line-height: 50rpx;
  padding: 50rpx auto;
  border: 1rpx solid #eeeeee;
  display:flex;
  align-items: center;
}
.item>icon:nth-of-type(1){
  border-radius: 50%;
  width: 50rpx;
  height: 50rpx;
  border: 1rpx solid #d9d9d9;
  margin: auto 20rpx;
  
  text-align: center;
}

.right{
  width: 95%;
  height: 80rpx;
  line-height: 80rpx;
  background: #5555ee;
  border-radius:0;
  color: white;
  font-size: 35rpx;
  position: fixed;
  bottom:50rpx;
  left:0;
  right:0;
  margin: auto; 
}