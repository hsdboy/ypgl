.jilu{
  background: white;
  height: 80rpx;
  color: #777777;
  line-height: 80rpx;
  display: flex;
  justify-content: space-around;
}
.jil{
  color: blue;
  border-bottom:1rpx solid blue; 
}
.item{
  font-size: 30rpx;
  line-height: 50rpx;
  padding: 50rpx auto;
  border: 1rpx solid #eeeeee;
  display:flex;
  align-items: center;
  background: white
}
ec-canvas {
  width: 100%;
  height: 100%;
}
.container {
  transition: all .5s;
  width: 100%;
  position: absolute;
  top: 300rpx;
  bottom: 0;
  left: 0;
  right: 0;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;
} 
