@import "../../style/iconfont.wxss";
.box {
  height: 88vh;
  background: #eee;
  overflow: auto;
}

.button {
  height: 12vh;
  background: #eee;
}
.title{
  margin: 20rpx;
}
.item{
  height: 50rpx;
  font-size: 30rpx;
  line-height: 50rpx;
  padding: 10rpx 10rpx;
  background: white;
  display: flex;
  align-items: center;
  border-radius:12rpx; 
  margin:10rpx;
  float:left;
}
.right{
  width: 95%;
  height: 80rpx;
  line-height: 80rpx;
  background: #5555ee;
  border-radius:0;
  color: white;
  font-size: 35rpx;
  position: fixed;
  bottom:50rpx;
  left:0;
  right:0;
  margin: auto; 
}